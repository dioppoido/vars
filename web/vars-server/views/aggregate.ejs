<!DOCTYPE html>

<html lang="ja">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Title-->
    <title>集計テスト - VARS -</title>

    <!-- Bootstrap Core CSS -->
    <link href="../public/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="../public/css/error.css" rel="stylesheet">

    <link href="../public/css/footer.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="../public/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href='http://fonts.googleapis.com/earlyaccess/notosansjapanese.css' rel='stylesheet' type='text/css'>

</head>
<body>

<!-- header -->
<section>
    <div class="container">
        <div class="row">

            <!-- navbar -->
            <nav class="navbar navbar-custom">
                <div class="navbar-header">
                    <!-- logo&title -->
                    <a class="navbar-brand page-scroll" href="/">
                        <div class="header-title">V A R S</div>
                    </a>
                </div>
            </nav>
            <div class="barbottom-color"></div>
        </div>
    </div>
</section>

<!-- Error -->
<section>
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1 col-lg-10 col-lg-offset-1">

                <p><h1>集計テスト</h1></p><br>

                <h2><%=error%></h2><br>

                <h2>イベント</h2>

                <h2>
                    <% if(event != "") { %>
                    Eventid : <%=event[0].Eventid%><br>
                    Eventname : <%=event[0].Eventname%><br>
                    <% } %>
                </h2><br>

                <h2>チーム</h2>

                <h2>
                    <% if(team != "") { %>
                       <% for(var i = 0 ; i < team.length; i++){ %>
                        Teamid : <%=team[i].Teamid%><br>
                        Teamname: <%=team[i].Teamname%><br>
                       <% } %>
                    <% } %>
                </h2><br>

                <h2>投票</h2>

                <h2>
                    <% if(vote != "") { %>
                        <% for(var i = 0 ; i < vote.length; i++){ %>
                                <%= vote[i].Votename%><br>
                        <% } %>
                    <% } %>
                </h2><br>



            </div><!-- col -->
        </div><!-- row -->
    </div><!-- container -->
</section>

<%- include('./footer.ejs') %>

<!-- jQuery -->
<script src="../public/js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="../public/js/bootstrap.min.js"></script>

<!-- Footer Fixed -->
<script src="../public/js/footerFixed.js"></script>

</body>
</html>
