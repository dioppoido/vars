<!DOCTYPE html>

<html lang="ja">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Title-->
    <title>EventCreate - VARS -</title>

    <!-- Bootstrap Core CSS -->
    <link href="../public/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="../public/css/eventcreate.css" rel="stylesheet">
    <link href="../public/css/header.css" rel="stylesheet">
    <link href="../public/css/footer.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="../public/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href='http://fonts.googleapis.com/earlyaccess/notosansjapanese.css' rel='stylesheet' type='text/css'>

</head>
<body>

<%- include('./header.ejs') %>

<section>
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1 col-lg-10 col-lg-offset-1">

                <div class="eventcreate">

                    <div class="event-title text-center"><i class="fa fa-plus-circle" aria-hidden="true"></i> イベント作成</div>

                    <form method="POST" name="eventinsert" action="/eventcreate">
                        <input type="hidden" name="eventname" value="イベントです">
                        <input type="hidden" name="overview" value="イベント概要です">
                        <input type="hidden" name="address" value="kd@st.kobedenshi.ac.jp">
                        <input type="hidden" name="displayname" value="多田君">
                        <input type="hidden" name="course" value="コース">
                        <input type="hidden" name="venue" value="学科">
                        <input type="hidden" name="date" value="11/21">
                        <input type="hidden" name="createstart" value="2016/11/08">
                        <input type="hidden" name="createfinish" value="2016/12/01">
                        <input type="hidden" name="votestart" value="2017/02/01">
                        <input type="hidden" name="votefinish" value="2017/02/01">
                        <input type="hidden" name="image" value="イベントです">
                    <button type="submit">送信</button>

                </form>
                    <div class="create-form">

                    </div>

                </div>


            </div><!-- col -->

        </div><!-- row -->
    </div><!-- container -->
</section>

<%- include('./footer.ejs') %>

<!-- jQuery -->
<script src="../public/js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="../public/js/bootstrap.min.js"></script>

<!-- Footer Fixed -->
<script src="../public/js/footerFixed.js"></script>

</body>
</html>
